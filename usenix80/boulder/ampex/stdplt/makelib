l=libP.a
rm -f $l
ar rv $l graph.o  fgrid.o saxis.o dispts.o y.tab.o pair.o domin.o donice.o
ar rv $l base.o dodisp.o axis.o  display.o grid.o  zinit.o minmax.o misc.o 
ar rv $l xdist.o ydist.o eval.o xhair.o where.o fwhere.o
ar rv $l frame.o rmframe.o _window.o dash.o fat.o
ar rv $l purge.o erase.o erasea.o
ar rv $l cfont.o cwidth.o crotate.o textf.o _move.o _draw.o
ar rv $l _xhair.o _rotate.o _scale.o _origin.o
ar rv $l prframe.o _cfname.o _path.o
ar rv $l _find.o _fmatch.o _cat.o _inv2.o
ar rv $l _domove.o _clip.o _err.o data.o
ar tv $l
