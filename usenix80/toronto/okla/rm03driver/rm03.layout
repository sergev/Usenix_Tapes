#define Csize	160	/* RM03 cylinder capacity */

	255*Csize,	3,	/* hm0 - 255 cylinders */
	 55*Csize,	258,	/* hm1 - 55 cylinders  */
	255*Csize,	313,	/* hm2 - 255 cylinders */
	255*Csize,	568,	/* hm3 - 255 cylinders */

	158*Csize,	3,	/* hm4 - first 158 cylinders of hm0 */
	97*Csize,	161,	/* hm5 - last 97 cylinders of hm0 */
	155*Csize,	258,	/* hm6 - 155 cylinders of hm[1,2] */

	   -1,		413,	/* hm7 - last 65535 blocks of hm[2,3]*/
	   -1,		3,	/* hm8 - 65535 blocks of hm[0,1,2] */

	155*Csize,	413,	/* hm9 - last 155 cylinders of hm2 */
	255*Csize,	413,	/* hm10 - 255 cylinders of hm[9,13] */
	155*Csize,	668,	/* hm11 - last 155 cylinders of hm3 */
	100*Csize,	313,	/* hm12 - first 100 cylinders of hm2 */
	100*Csize,	568,	/* hm13 - first 100 cylinders of hm3 */
	0,		0,	/* hm14 - undefined */

	3*Csize,	0,	/* hm15 - 3 cylinder deadstart device */
