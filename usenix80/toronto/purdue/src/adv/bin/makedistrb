cat advent.f4p initad.f4p rkb.f4p admotd.f4p asub.f4p iosub.f4p ishft.mac >full
cat advent.txt >>full
/u/reevesr/bin/detab <full >full.1
/u/reevesr/bin/cards <full.1 >c80
chmod 600 c80
: rm full
: rm full.1
date
echo advent dump complete
