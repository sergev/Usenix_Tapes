: make -- make all commands except those in subdirectories.

cc -s -n -O cat.c -o cat; chmod 755 cat; echo mv cat /bin when ready
cc -s -n -O cp.c -o cp; chmod 755 cp; echo mv cp /bin when ready
cc -s -n -O echo.c -o echo; chmod 755 echo; echo mv echo /bin when ready
cc -s -n -O exit.c -o exit; chmod 755 exit; echo mv exit /bin when ready
cc -s -n -O getty.c -o getty; chmod 1700 getty; echo mv getty /etc when ready
cc -s -n -O ln.c -o ln; chmod 755 ln; echo mv ln /bin when ready
cc -s -n -O login.c -o login; chmod 4711 login; echo mv login /bin when ready
cc -s -n -O mail.c -o mail; chmod 4711 mail; echo mv mail /bin when ready
cc -s -n -O mesg.c -o mesg; chmod 755 mesg; echo mv mesg /bin when ready
cc -s mkconf.c -o mkconf; chmod 755 mkconf; echo mv mkconf /usr/bin when ready
cc -s -n -O mv.c -o mv; chmod 755 mv; echo mv mv /bin when ready
cc -s -n -O opr.c -o opr; chmod 755 opr; echo mv opr /bin when ready
cc -s -O -f -DPLOT prof.c -lg -lm -o prof; chmod 755 prof; echo mv prof /usr/bin when ready
cc -s -n -O sleep.c -o sleep; chmod 755 sleep; echo mv sleep /bin when ready
cc -s -n -O stty.c -o stty; chmod 755 stty; echo mv stty /bin when ready
cc -s -n -O sync.c -o sync; chmod 755 sync; echo mv sync /bin when ready
cc -s -n -O tee.c -o tee; chmod 755 tee; echo mv tee /bin when ready
cc -s -n -O wc.c -o wc; chmod 755 wc; echo mv wc /bin when ready
