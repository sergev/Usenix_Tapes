=********************************************************************=
=                                                                    =
=                      ROOT-BARS SEMANTIC TABLE                      =
=                                                                    =
=********************************************************************=
=        =                                                           =
=   SS   =                          OUTPUT                           =
=        =                                                           =
= 'S'    = '&(a)';                                                   =
= 'null' = '';                                                       =
= 'ep'   = 'LET S#(a+) := ADJUST(SELECT(&(b),&(c),&(d)),1)\n';       =
= 'c1'   = 'LET S#(a+) := INTER(&(b)&(c),&(d)&(e))\n';               =
= 'c2'   = 'LET S#(a+) := UNION(&(b)&(c),&(d)&(e))\n';               =
= 'c3'   = 'LET S#(a+) := RLCOMP(TYPE(1),&(b)&(c))\n';               =
= 'hc'   = 'LET S#(a+) := IDENT(&(c)&(d))\n';                        =
= 'qs'   = 'LET Q := QUALIFY(&(b))\n';                               =
=        =                                                           =
=********************************************************************=


















































































































