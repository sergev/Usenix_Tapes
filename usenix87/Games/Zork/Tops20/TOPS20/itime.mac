	TITLE ITIME
	ENTRY ITIME,IDATE

P=17
AP=16
T1=1
T2=2

ITIME::	PUSH	P,T1
	PUSH	P,T2
	MSTIME	T1,
	IDIVI	T1,^D1000
	IDIVI	T1,^D60
	MOVEM	T2,@2(AP)
	IDIVI	T1,^D60
	MOVEM	T2,@1(AP)
	MOVEM	T1,@0(AP)
RETR:	POP	P,T2
	POP	P,T1
	POPJ	P,

IDATE:	PUSH	P,T1
	PUSH	P,T2
	DATE	T1,
	IDIVI	T1,^D31
	AOS	T2
	MOVEM	T2,@2(AP)
	IDIVI	T1,^D12
	AOS	T2
	MOVEM	T2,@1(AP)
	ADDI	T1,^D64
	MOVEM	T1,@0(AP)
	JRST	RETR

	END
