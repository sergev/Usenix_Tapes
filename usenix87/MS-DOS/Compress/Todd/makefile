comsmlpc.exe: compress.c
	msc compress,compress/FPi/Ze/Gs/Ot;
	link compress,comsmlpc/NOI;

combigpc.exe: compress.c
	msc compress,compress/DBIG/FPi/Ze/Gs/Ot;
	link compress,combigpc.big/NOI;
	exepack combigpc.big combigpc.exe

comsmlat.exe: compress.c
	msc compress,compress/FPi/Ze/Gs/Ot/G2;
	link compress,comsmlat/NOI;


combigat.exe: compress.c
	msc compress,compress/DBIG/FPi/Ze/Gs/Ot/G2;
	link compress,combigat.big/NOI;
	exepack combigat.big combigat.exe
