vshmode select
quitchar [
pagechar `
entertext display
helpfile ""
column 10
window 11
enterpath :/usr/spool:/src:~
? display /usr/lib/vsh/genhelp.dan
= pexec ls $
; xecute
% exec /bin/csh
# exec ;su root vsh -
/ file
@ exec ;rm -r $
& xecute ;cc -O -c $ &
( exec ;pack $
) exec ;unpack $
~ file
` file .
, pexec sh -c ??Command:
: exec ;chmod "??New mode:" $
A exec ;adb $
B pexec od $
F pexec df
H home
I pexec tar tvf $
J pexec nroff -ms $
K exec ;notes -i *.*
P pexec ps -la
Q pexec du $
T pexec tail -19 $
U exec ;su
V chain vsh -
W pexec who
X exec $
u pexec uustat
v exec ;vi $
w exec ;mail
x pexec size $
y pexec file $
z exec ;jove $
