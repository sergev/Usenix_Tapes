  /in {72 mul } def 
 /intt { 2 setlinewidth 0 setgray newpath 0 0 moveto }def
 /drawgraybox { .4 setgray  
 0 in 4 in lineto  
 .625 in 4 in lineto  
 .625 in 0 in lineto 
 0 0 lineto  
 fill intt } def 
 /drawtapebox { 2 setlinecap .625 in 0 moveto .625 in 4 in lineto stroke 
 0 0 moveto 0 4 in lineto stroke 
 1.125 in 0 moveto 1.125 in 4 in lineto stroke 
 3.75 in 0 moveto 3.75 in 4 in lineto stroke 
 0 0  moveto 3.75 in 0 lineto  stroke 
 .625 in 2 in moveto 3.75 in 2 in lineto stroke 
 0 4 in moveto 3.75 in 4 in lineto stroke 
 0 setlinecap 
 } def 
 /makeit { intt drawgraybox drawtapebox 3.75 in 0 in translate 90 rotate } def 
 /LM 7 def 
 /LM2 155 def 
 /title1 { newpath LM 2.9 in moveto } def 
 /title2 { newpath LM2 2.9 in moveto } def 
 /stitle1 { newpath LM 2.7 in moveto } def 
 /stitle2 { newpath LM2 2.7 in moveto } def 
 /newline { currentpoint 12 sub LM exch moveto pop } def 
 /new2 { currentpoint 12 sub LM2 exch moveto pop } def 
 /titlefont { /Helvetica-Bold findfont 14 scalefont setfont} def 
/normalfont { /Helvetica findfont 10 scalefont setfont} def 
 /songs1 { newpath LM 2.5 in moveto } def 
 /songs2 { newpath LM2 2.5 in moveto } def 
 .5 in 1.0 in translate 
 makeit 
 titlefont
title1 (Your Artist ) show
stitle1 (Your Album ) show
 normalfont
 songs1
newline (your first song ) show
newline (your second song ) show
 titlefont 
 title2 (Second Artist) show
stitle2 (Second Album)show
 normalfont 
  songs2
new2 (second Album's songs ) show
